							object_id: editor.pages[i].objects[j].id
						})
					}else if(editor.pages[i].objects[j].image_details.id_pixlr){
						result.push({
							id: editor.pages[i].objects[j].image_details.id_pixlr,
							type: 'pixlr',
							object_id: editor.pages[i].objects[j].id
						})
					}
